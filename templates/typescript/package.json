        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache"
    },
    "dependencies": {},
    "devDependencies": {
        "@eslint/js": "^9.22.0",
        "@types/node": "^22.13.10",
        "eslint": "^9.22.0",
        "typescript-eslint": "^8.15.0",
        "prettier": "^3.3.2",
        "typescript": "^5.6.3"
    },
    "packageManager": "npm@10.9.2"
}
